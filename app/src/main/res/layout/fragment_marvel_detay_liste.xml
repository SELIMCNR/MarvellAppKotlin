<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.MarvelDetayListe">

    <ImageView
        android:id="@+id/marvelImagedetay"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/ic_launcher_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_editor_absoluteX="105dp"
        app:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/marvelIsimdetay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Marvel isim bilgilerini giriniz"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marvelImagedetay"
        app:layout_editor_absoluteX="177dp"
        app:layout_editor_absoluteY="236dp" />

    <TextView
        android:id="@+id/marvelKahramanözellikdetay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Marvel kahraman bilgilerini giriniz"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marvelIsimdetay"
        app:layout_editor_absoluteX="177dp"
        app:layout_editor_absoluteY="236dp" />

    <TextView
        android:id="@+id/marvelKahramanyasadigiyerdetay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Marvel kahraman şehir bilgilerini giriniz"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marvelKahramanözellikdetay"
        app:layout_editor_absoluteX="177dp"
        app:layout_editor_absoluteY="236dp" />

    <TextView
        android:id="@+id/marvelKahramanyas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Marvel kahraman yas bilgilerini giriniz"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marvelKahramanyasadigiyerdetay"
        app:layout_editor_absoluteX="177dp"
        app:layout_editor_absoluteY="236dp" />

    <TextView
        android:id="@+id/marvelKahramanCinsiyet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Marvel kahraman cinsiyet bilgilerini giriniz"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marvelKahramanyas"
        app:layout_editor_absoluteX="177dp"
        app:layout_editor_absoluteY="236dp" />

</androidx.constraintlayout.widget.ConstraintLayout>